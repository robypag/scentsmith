{
    "name": "scentsmith",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev --turbopack",
        "dev:all": "concurrently \"npm run dev\" \"npm run dev:workers\"",
        "dev:workers": "tsx scripts/start-workers.ts",
        "build": "next build",
        "build:all": "next build && npm run build:workers",
        "build:workers": "tsc --project tsconfig.workers.json",
        "start": "next start",
        "start:all": "concurrently \"npm run start\" \"npm run start:workers\"",
        "start:workers": "node dist/scripts/start-workers.js",
        "lint": "next lint",
        "db:generate": "drizzle-kit generate",
        "db:migrate": "tsx src/lib/db/migrate.ts",
        "db:drop": "drizzle-kit drop",
        "db:pull": "drizzle-kit introspect",
        "db:push": "drizzle-kit push",
        "db:studio": "drizzle-kit studio",
        "db:check": "drizzle-kit check"
    },
    "dependencies": {
        "@ai-sdk/openai": "^1.3.22",
        "@langchain/community": "^0.3.44",
        "@langchain/core": "^0.3.57",
        "@langchain/openai": "^0.5.11",
        "@radix-ui/react-dialog": "^1.1.14",
        "@radix-ui/react-label": "^2.1.7",
        "@radix-ui/react-progress": "^1.1.7",
        "@radix-ui/react-select": "^2.2.5",
        "@radix-ui/react-separator": "^1.1.7",
        "@radix-ui/react-slot": "^1.2.3",
        "@radix-ui/react-tabs": "^1.1.12",
        "@radix-ui/react-tooltip": "^1.2.7",
        "@types/ioredis": "^4.28.10",
        "@types/pg": "^8.15.2",
        "ai": "^4.3.16",
        "bullmq": "^5.53.0",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "dotenv": "^16.5.0",
        "drizzle-kit": "^0.31.1",
        "drizzle-orm": "^0.43.1",
        "drizzle-zod": "^0.8.2",
        "ioredis": "^5.6.1",
        "langchain": "^0.3.27",
        "lucide-react": "^0.511.0",
        "nanoid": "^5.1.5",
        "next": "15.3.2",
        "next-auth": "^4.24.11",
        "next-themes": "^0.4.6",
        "pdf-parse": "^1.1.1",
        "pg": "^8.16.0",
        "pgvector": "^0.2.1",
        "postcss": "^8.5.3",
        "postgres": "^3.4.7",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "sonner": "^2.0.3",
        "tailwind-merge": "^3.3.0",
        "tailwindcss-animate": "^1.0.7",
        "zod": "^3.25.28",
        "module-alias": "^2.2.3"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3",
        "@types/node": "^20",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "autoprefixer": "^10.4.20",
        "concurrently": "^9.1.2",
        "eslint": "^9",
        "eslint-config-next": "15.3.2",
        "tailwindcss": "^3.4.13",
        "ts-node": "^10.9.1",
        "tsx": "^4.7.1",
        "tw-animate-css": "^1.3.0",
        "typescript": "^5"
    }
